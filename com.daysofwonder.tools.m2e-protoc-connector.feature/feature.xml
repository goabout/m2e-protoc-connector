<feature
      id="com.daysofwonder.tools.m2e-protoc-connector.feature"
      label="Maven SCM Handler for EGit"
      version="1.0.0.20111130-1035"
      provider-name="Days of Wonder, Inc."
      plugin="com.daysofwonder.tools.m2e-protoc-connector">

   <description url="http://m2eclipse.sonatype.org/">
      Build maven projects using Google Protobuf plugin under Eclipse
   </description>

   <copyright>
      %copyright
   </copyright>

   <license url="%licenseURL">
      %license
   </license>

   <url>
      <update label="Maven Integration Extras" url="http://m2eclipse.sonatype.org/sites/m2e-extras/"/>
   </url>

   <requires>
      <import feature="org.eclipse.platform" version="3.4.0" match="greaterOrEqual"/>
      <import feature="org.eclipse.m2e.feature"/>
      <import feature="org.eclipse.egit"/>
      <import feature="org.eclipse.jgit"/>
   </requires>

   <plugin
         id="com.daysofwonder.tools.m2e-protoc-connector"
         download-size="0"
         install-size="0"
         version="0.0.0"
         unpack="false"/>

</feature>
